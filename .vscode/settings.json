{
  "files.associations": {
    "*.install": "php",
    "*.module": "php",
    "*.profile": "php",
    "*.theme": "php",
    "*.inc": "php"
  },
  "git.autoRepositoryDetection": "subFolders",
  "git.repositoryScanMaxDepth": 5,
  "git.repositoryScanIgnoredFolders": [
    "node_modules",
    "vendor",
    "core",
    "sites"
  ]
}
